{
  "name": "superhero-wallet",
  "version": "0.6.0",
  "description": "Superhero wallet",
  "author": "Superhero",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:js": "eslint --ext .js,.vue src tests",
    "lint:css": "stylelint src/**/*.{css,scss,vue}",
    "lint:commit": "commitlint --from develop --verbose",
    "lint": "npm run lint:commit && npm run lint:js && npm run lint:css",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules -d",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch",
    "test": "start-server-and-test start http://localhost:4999 start:tests",
    "start": "npm run build:tests && serve -l 4999",
    "start:ci": "serve",
    "start:tests": "cypress run",
    "gen:cordova-resources": "cordova-res --icon-background-source '#090909'",
    "build:Testnet": "cross-env NETWORK=Testnet npm run build",
    "watch:dev:Testnet": "cross-env NETWORK=Testnet npm run watch:dev",
    "watch:tests": "cross-env NETWORK=Testnet RUNNING_IN_TESTS=true npm run watch:dev",
    "build:tests": "cross-env NETWORK=Testnet RUNNING_IN_TESTS=true npm run build:dev",
    "release": "standard-version"
  },
  "dependencies": {
    "@aeternity/aepp-components-3": "3.0.0-alpha.9",
    "@aeternity/aepp-sdk": "^7.7.0",
    "@aeternity/bip39": "^0.1.0",
    "@aeternity/hd-wallet": "^0.2.0",
    "@zxing/library": "^0.18.3",
    "aepp-raendom": "github:aeternity/aepp-raendom#feature/layout-superhero",
    "aeternity-fungible-token": "github:mradkov/aeternity-fungible-token",
    "bignumber.js": "^9.0.1",
    "cordova-android": "^9.0.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-headercolor": "^1.0.0",
    "cordova-plugin-qrscanner": "^3.0.1",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "detect-browser": "^5.2.0",
    "fontsource-ibm-plex-sans": "^3.1.5",
    "lodash-es": "^4.17.20",
    "qrcode.vue": "^1.7.0",
    "rxjs": "^6.6.3",
    "rxjs-etc": "^10.5.1",
    "tipping-contract": "github:aeternity/tipping-contract#develop",
    "uuid": "^3.3.3",
    "validator": "^13.5.2",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-flickity": "^1.2.1",
    "vue-i18n": "^8.22.3",
    "vue-router": "^3.4.9",
    "vue-rx": "^6.2.0",
    "vue-tour": "^1.5.0",
    "vuex": "^3.6.0",
    "vuex-router-sync": "^5.0.0",
    "webextension-polyfill": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "archiver": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cordova": "^10.0.0",
    "cordova-plugin-openwith": "git+https://github.com/aeternity/cordova-plugin-openwith.git",
